clearsysmsg
clearignore 

while findtype 29833 backpack as found
    dclick found
    #remove traps
    while not insysmsg "successfully"
        if insysmsg "devoid" 
            break
        endif
        wait 250
        gumpresponse 7
        wait 3500
    endwhile
    
    while not insysmsg "successfully"
        if insysmsg "devoid" 
            break
        endif
        wait 250
        gumpresponse 4
        wait 3500
    endwhile
    ignore found
endwhile
