@clearignore
overhead "Target Source"
@setvar! containerToSplit
overhead "Target Destination"
@setvar! containerToSplitTwo
if not listexists nonStackItems
  createlist nonStackItems
  pushlist nonStackItems 3985
  pushlist nonStackItems 3836
  pushlist nonStackItems 8826
  pushlist nonStackItems 5356 
  pushlist nonStackItems 24434 
  pushlist nonStackItems 22336 
  pushlist nonStackItems 17087
  pushlist nonStackItems 43206
endif

if not listexists stackItems
  createlist stackItems
  pushlist stackItems 29030 
  pushlist stackItems 3827 
  pushlist stackItems 3821 
  pushlist stackItems 2539 
  pushlist stackItems 45315 
endif


overhead 'Dropping Non-Stackable Items' 100

foreach x in nonStackItems
  while counttype x containerToSplit > 1 as this
    while findtype x containerToSplit as this
        sysmsg 'Lifting...' 44
        lift this 1
        drop containerToSplitTwo 17 104 0
        wait 650
        @ignore this
    endwhile
  endwhile
endfor

overhead 'Dropping Stackable Items' 100

foreach x in stackItems
  while counttype x containerToSplit > 1 as this
    while findtype x containerToSplit as this
        sysmsg 'Lifting...' 44
        lift this 60000
        drop containerToSplitTwo -1 -1
        wait 650
        @ignore this
    endwhile
  endwhile
endfor
@clearignore