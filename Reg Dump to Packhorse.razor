
if varexist noidBag
    #var exists, make sure we can reach it
    if find noidBag ground any any 2
        overhead 'Packie Set Already, dumping'
    else 
        overhead 'Where did packie go!' 33
        #We lost packie!  Retarget
        sysmsg  'Target Packie'
        overhead 'Target Dump'
        setvar noidBag
    endif
else 
    sysmsg  'Target Packie'
    overhead 'Target Dump'
    setvar noidBag
endif

clearignore  
createlist regTypes
clearlist regTypes
@pushlist 'regTypes' 'Blood Moss'
@pushlist 'regTypes' 'Black Pearl%s%'
@pushlist 'regTypes' 'Ginseng'
@pushlist 'regTypes' 'Garlic'
@pushlist 'regTypes' 'Sulfurous Ash'
@pushlist 'regTypes' "Spider's Silk" //SS
@pushlist 'regTypes' 'Nightshade'
@pushlist 'regTypes' 'Mandrake Root%s%' //MR


foreach x in regTypes
  while findtype x backpack any any 1 as found
    @hotkey 'set last target'
    @target found
    @setvar myVariable lasttarget
    wait 100
    lift myVariable 9999
    drop noidBag -1 -1 0 
    @ignore myVariable
    wait 500
  endwhile
endfor