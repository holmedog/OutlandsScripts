
if varexist noidBag
    #var exists, make sure we can reach it
    if find noidBag ground any any 2
        overhead 'Packie Set Already, dumping'
    else 
        overhead 'Where did packie go!' 33
        #We lost packie!  Retarget
        sysmsg  'Target Packie'
        overhead 'Target Dump'
        setvar noidBag
    endif
else 
    sysmsg  'Target Packie'
    overhead 'Target Dump'
    setvar noidBag
endif

