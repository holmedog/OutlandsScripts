                hotkey '> Interrupt'
                break
            else
                skill "Discordance" 
                wait 50
            endif 
        endwhile
        if targetexists
            target myKillTarget
            settimer discoTimer 0
        endif
        wait 250
        if insysmsg "You play successfully" 
            #this is reset after a new target is acquired 
            @setvar! discordOn 1
        endif
    endif
    
    if skill "poisoning" >= 100 or castPoisonAnyway = 1 and timer meditiationTimer >= cdMeditationMini

