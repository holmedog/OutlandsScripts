                            target 'self'
                        endif    

                        #drop gold on vault
                        if findtype 41518 ground as myVault
                            while findtype 3821 self as 'myGold'
                                lift myGold 60000
                                drop '0x40820133' -1 -1 0 
                                wait 600
                            endwhile
