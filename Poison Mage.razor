                        wait 600

                        #"Starting Organizers"
                        organizer 1
                        while queued
                            wait 1000
                        endwhile
                        organizer 2
                        while queued
                            wait 1000
                        endwhile
                        organizer 3
                        while queued
                            wait 1000
                        endwhile
                        organizer 4
                        while queued
                            wait 1000
                        endwhile
                        organizer 5
                        while queued
                            wait 1000
                        endwhile
                        organizer 6
                        while queued
                            wait 1000
                        endwhile
                        organizer 7
                        while queued
                            wait 1000
                        endwhile
                        organizer 8
                        while queued
                            wait 1000
                        endwhile
                        organizer 9
