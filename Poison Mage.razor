            if "Lethal" in desc2
                @setvar! isLethalPoison 1
                break
            elseif deadlyOK = 1 and "Deadly" in desc2
                @setvar! isLethalPoison 1
                break
            elseif castPoisonAnyway = 1 and "Ticks" in desc2
                @setvar! isLethalPoison 1
                break
            elseif castPoisonAnyway = 1 and "Poison" in desc2
                @setvar! isLethalPoison 1
                break
