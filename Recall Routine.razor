

clearignore 
unsetvar 'gfto'
while findtype 8901 'backpack' as 'runeBook'
    getlabel 'runeBook' 'runeBookLabel'
    if 'Home' in 'runeBookLabel'
        setvar 'gtfo' 'runeBook'
        overhead 'Set recall to rune book'
    endif
    @ignore 'runeBook'
endwhile
while findtype 8901 'backpack' as 'recallRune'
    getlabel 'recallRune' 'recallRuneLabel'
    if 'Home' in 'recallRuneLabel'
        setvar 'gfto' 'recallRune'
        overhead 'Set recall to rune'
    endif
    @ignore 'runeBook'
endwhile
  
if not varexist 'gtfo'
    overhead 'You need a book or rune named Home' 33
    wait 500
    overhead 'You need a book or rune named Home' 33
    wait 500
    overhead 'You need a book or rune named Home' 33
    wait 500
elseif skill magery > 1
    cast 'recall'
    wft 5000
    target 'gtfo'
elseif findtype 8012 'backpack'as recallScrolls
    dclick 'recallScrolls'
    wft 5000
    target 'gtfo'
else
    dclick 'gtfo'
    wait 250
    gumpresponse 2    
endif