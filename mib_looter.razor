overhead "Target Box"
setvar! lootBox
dclick lootBox
wait 500
overhead "Target Dump"
setvar! lootDump
clearignore 
while findtype 3702 lootBox as foundBag
    dclick foundBag
    wait 500
    while findtype 5357 foundBag as found
        lift found 1
        drop lootDump 6 -1 -1 0 
        wait  500
        overhead "LIFT"
        ignore found
    endwhile
    ignore foundBag
endwhile
    